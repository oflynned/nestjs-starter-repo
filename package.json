{
  "name": "nestjs-starter-api",
  "version": "0.0.0",
  "private": false,
  "scripts": {
    "assets:copy": "mkdir -p ./dist/assets && cp -r ./src/assets/*.graphql ./dist/assets",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "deps:lint": "npm-check-updates",
    "deps:lint:minor": "npm-check-updates --target minor",
    "deps:lint:patch": "npm-check-updates --target patch",
    "deps:sort": "sort-package-json",
    "deps:upgrade": "npm-check-updates -u",
    "deps:upgrade:minor": "npm-check-updates -u --target minor",
    "deps:upgrade:patch": "npm-check-updates -u --target patch",
    "format": "prettier --write \"**/*.ts\"",
    "format:staged": "lint-staged",
    "format:types": "prettier --write \"src/core/types/*\"",
    "postinstall": "husky install",
    "lint": "eslint \"src/**/*.ts\"",
    "lint:fix": "eslint \"src/**/*.ts\" --fix",
    "migrate:create": "mikro-orm migration:create",
    "migrate:create:initial": "mikro-orm migration:create --initial",
    "migrate:down": "mikro-orm migration:down",
    "migrate:list": "mikro-orm migration:list",
    "migrate:pending": "mikro-orm migration:pending",
    "migrate:reset": "mikro-orm migration:down --to 0",
    "migrate:up": "mikro-orm migration:up",
    "orm:cache:generate": "mikro-orm cache:generate",
    "prestart": "npm run types && npm run assets:copy",
    "start": "npm run migrate:pending && nest start --watch",
    "start:dev": "nest start --debug --watch",
    "start:e2e": "echo \"Firing up mock server...\" && ts-node src/e2e/e2e.main.ts",
    "start:e2e:watch": "nodemon --ignore '**/*.e2e-spec.ts' src/e2e/e2e.main.ts",
    "test": "npm run test:unit",
    "test:e2e": "jest --config jest.config.e2e.js --runInBand --detectOpenHandles",
    "test:e2e:watch": "npm run test:e2e -- --watch",
    "test:unit": "jest --config jest.config.unit.js",
    "test:unit:cov": "npm test -- --coverage",
    "test:unit:watch": "npm run test:unit -- --watch",
    "ts-node": "ts-node -r tsconfig-paths/register -P ./tsconfig.json",
    "typecheck": "tsc --noEmit --project tsconfig.json",
    "types": "npm run types:merge && npm run types:gen",
    "types:gen": "ts-node src/tooling/generate-typings.ts",
    "types:merge": "ts-node src/tooling/merge-definitions.ts"
  },
  "lint-staged": {
    "package.json": [
      "sort-package-json"
    ],
    "*.ts": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "dependencies": {
    "@bull-board/express": "3.7.0",
    "@eropple/nestjs-correlation-id": "1.0.1",
    "@mikro-orm/core": "4.5.9",
    "@mikro-orm/migrations": "4.5.9",
    "@mikro-orm/nestjs": "4.3.1",
    "@mikro-orm/postgresql": "4.5.9",
    "@nestjs/bull": "0.4.2",
    "@nestjs/common": "8.2.2",
    "@nestjs/config": "1.1.0",
    "@nestjs/core": "8.2.2",
    "@nestjs/graphql": "9.1.1",
    "@nestjs/platform-express": "8.2.2",
    "@sentry/integrations": "6.15.0",
    "@sentry/node": "6.15.0",
    "@sentry/tracing": "6.15.0",
    "apollo-server-express": "3.5.0",
    "apollo-server-plugin-operation-registry": "3.4.0",
    "bull": "3.22.12",
    "bull-board": "2.1.3",
    "cache-manager": "3.6.0",
    "class-transformer": "0.4.0",
    "date-fns": "2.26.0",
    "express-basic-auth": "1.2.0",
    "firebase-admin": "9.12.0",
    "graphql": "15.7.2",
    "graphql-scalars": "1.13.6",
    "joi": "17.4.2",
    "mikro-orm": "4.5.9",
    "nestjs-firebase": "8.1.0",
    "parse-redis-url-simple": "1.0.2",
    "pg": "8.7.1",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.2",
    "rxjs": "7.4.0",
    "uuid": "8.3.2"
  },
  "devDependencies": {
    "@graphql-tools/load-files": "6.5.2",
    "@graphql-tools/merge": "8.2.1",
    "@mikro-orm/cli": "4.5.9",
    "@nestjs/cli": "8.1.5",
    "@nestjs/schematics": "8.0.5",
    "@nestjs/testing": "8.2.2",
    "@types/bull": "3.15.5",
    "@types/cache-manager": "3.4.2",
    "@types/express": "4.17.13",
    "@types/faker": "5.5.9",
    "@types/jest": "27.0.3",
    "@types/jsonwebtoken": "8.5.6",
    "@types/node": "16.11.9",
    "@types/shelljs": "0.8.9",
    "@types/statsd-client": "0.4.3",
    "@types/supertest": "2.0.11",
    "@types/uuid": "8.3.3",
    "@typescript-eslint/eslint-plugin": "5.4.0",
    "@typescript-eslint/parser": "5.4.0",
    "apollo-server-core": "^3.0.0",
    "dotenv": "10.0.0",
    "eslint": "8.2.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.25.3",
    "faker": "5.5.3",
    "graphql-tools": "8.2.0",
    "husky": "7.0.4",
    "jest": "27.3.1",
    "jest-mock-extended": "2.0.4",
    "jsonwebtoken": "8.5.1",
    "lint-staged": "12.0.3",
    "module-alias": "2.2.2",
    "nodemon": "2.0.15",
    "npm-check-updates": "12.0.2",
    "prettier": "2.4.1",
    "prettier-plugin-organize-imports": "2.3.4",
    "readline-sync": "1.4.10",
    "shelljs": "0.8.4",
    "sort-package-json": "1.53.1",
    "superagent": "6.1.0",
    "ts-jest": "27.0.7",
    "ts-loader": "9.2.6",
    "ts-morph": "12.2.0",
    "ts-node": "10.4.0",
    "tsconfig-paths": "3.11.0",
    "typescript": "4.5.2",
    "webpack": "5.64.1",
    "webpack-merge": "5.8.0",
    "webpack-node-externals": "3.0.0"
  },
  "engines": {
    "node": ">=16",
    "npm": ">=8",
    "pnpm": "please-use-npm",
    "yarn": "please-use-npm"
  },
  "mikro-orm": {
    "useTsNode": true,
    "configPaths": [
      "./src/mikro-orm.config.ts",
      "./dist/mikro-orm.config.js"
    ]
  }
}
